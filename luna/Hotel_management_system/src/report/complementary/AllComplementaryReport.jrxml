			<![CDATA[SELECT bill_item_info.menu_id,menu.menu_name,bill_item_info.bill_id,bill_item_info.quantity,item_unit.unit_name,bill.bill_datetime,menu.retail_price FROM bill_item_info INNER JOIN bill ON bill_item_info.bill_id = bill.bill_id INNER JOIN menu ON bill_item_info.menu_id = menu.menu_id INNER JOIN item_unit ON menu.unit_id = item_unit.unit_id  WHERE bill_item_info.complimentary_type = 1 AND (bill.bill_datetime >= $P{frm}) AND (bill.bill_datetime <= $P{to}) and menu.department_id = $P{depId}]]>
